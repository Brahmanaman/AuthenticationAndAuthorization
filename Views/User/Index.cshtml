
@{
    ViewBag.Title = "Index";
}

<h2>This is a Index of User Controller</h2>

@if (TempData["Message"] != null)
{
    <h3 class="text-success">@TempData["Message"]</h3>
}

@if (Request.Cookies["UserName"] != null)
{
    <h1>@Request.Cookies["UserName"].Value.ToString()</h1>

}

@if (Request.Cookies["UserEmail"] != null)
{
    <h1>@Request.Cookies["UserEmail"].Value.ToString()</h1>
}

@if (Session["UserEmail"] != null)
{
    <h1>@Session["UserEmail"].ToString()</h1>
}
